<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Description>A state machine library utilizing Expression trees</Description>
    <PackageProjectUrl>https://github.com/snargledorf/FastState</PackageProjectUrl>
    <RepositoryUrl>https://github.com/snargledorf/FastState</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>state machine</PackageTags>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <Version>1.1.0</Version>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\README.md">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

</Project>
